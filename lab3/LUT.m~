function LUT(obraz, przekodowanie)
    A = intlut(uint8(img), przekodowanie);
    
    figure(1)
    title('przekodowanie');
    subplot(2, 2, 1);
    plot(przekodowanie);
    
    figure(2)
    title('oryginal');
    subplot(2, 2, 2);
    imshow(img);
    
    
    figure(3)
    title('przeksztalcony');
    subplot(2, 2, 3);
    imshow(A);

end

