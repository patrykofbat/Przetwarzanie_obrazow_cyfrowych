close all;
clearvars;
clc;

img = imread('morfologia/fingerprint.bmp');
N = Inf;
img_2 = bwmorph(img, 'thin', N);

figure(1);
subplot(1,2,1);
imshow(img);
subplot(1,2,2);
imshow(img_2);

img = imread('morfologia/kosc.bmp');
img_2 = bwmorph(img, 'skel', N);

figure(2);
subplot(1,2,1);
imshow(img);
subplot(1,2,2);
imshow(img_2);

img = imread('morfologia/text.bmp');

